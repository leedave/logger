{
    "name": "leedch/logger",
    "type": "library",
    "description": "Basic setup used for my logging features",
    "keywords": [
        "leedch",
        "log",
        "phpunit"
    ],
    "homepage": "https://github.com/leedave/logger.git",
    "license": "MIT",
    "authors": [
        {
            "name": "David Lee",
            "email": "leed@leed.ch",
            "homepage": "https://www.leed.ch/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.0",
        "leedch/validator": "1.*.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Leedch\\Logger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leedch\\Logger\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}